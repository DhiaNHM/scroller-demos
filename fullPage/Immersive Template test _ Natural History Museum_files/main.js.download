                        } else {
                            changeColor(curSvg, $this.data('base-color'));

                            $this.closest('a').hover(function(e) {

                                if(e.type === 'mouseenter') {
                                    changeColor(curSvg, hoverColor);
                                }

                                if (e.type === 'mouseleave') {
                                    changeColor(curSvg, baseColor);
                                }
                            });
                        }
                    });
                }
            });

        });
